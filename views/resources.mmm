<table class="table table-bordered table-striped">
	<thead>
		<th>path</th>
		<th>methods</th>
		<th>headers</th>
	<thead>	
	{{# resources }}
	<tr>
		<td>
			{{ path }}
		</td>
		<td>
			<ul>
				{{# methods }}
				<li>{{.}}</li>
				{{/ methods }}
			</ul>
		</td>
		<td>
			<ul>
				{{# headers }}
				<li>{{ name }}: {{ value }}</li>
				{{/ headers }}
			</ul>
		</td>
	</tr>
	{{/ resources }}
</table>